class_):\n","    logA = self.logAs[class_]\n","    logpi = self.logpis[class_]\n","\n","    last_idx = None\n","    logprob = 0\n","    for idx in input_:\n","      if last_idx is None:\n","        # it's the first token\n","        logprob += logpi[idx]\n","      else:\n","        logprob += logA[last_idx, idx]\n","      \n","      # update last_idx\n","      last_idx = idx\n","    \n","    return logprob\n","  \n","  def predict(self, inputs):\n","    predictions = np.zeros(len(inputs))\n","    for i, input_ in enumerate(inputs):\n","      posteriors = [self._compute_log_likelihood(input_, c) + self.logpriors[c] \\\n","             for c in range(self.K)]\n","      pred = np.argmax(posteriors)\n","      predictions[i] = pred\n","    return predictions"],"metadata":{"id":"fTnLjJCORyuE","executionInfo":{"status":"ok","timestamp":1656571080363,"user_tz":420,"elapsed":7,"user":{"displayName":"Shalini Asopa","userId":"00276784227062791281"}}},"execution_count":48,"outputs":[]},{"cell_type":"code","source":["# each array must be in order since classes are assumed to index these lists\n","clf = Classifier([logA0, logA1], [logpi0, logpi1], [logp0, logp1])"],"metadata":{"id":"ji7zpOZNTMdn","executionInfo":{"status":"ok","timestamp":1656571081500,"user_tz":420,"elapsed":6,"user":{"displayName":"Shalini Asopa","userId":"00276784227062791281"}}},"execution_count":49,"outputs":[]},{"cell_type":"code","source":["Ptrain = clf.predict(train_text_int)\n","print(f\"Train acc: {np.mean(Ptrain == Ytrain)}\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"ooM43OHRTxxB","executionInfo":{"status":"ok","timestamp":1656571082619,"user_tz":420,"elapsed":8,"user":{"displayName":"Shalini Asopa","userId":"00276784227062791281"}},"outputId":"c445cb48-f20f-472e-eccb-3b2b2f563bd8"},"execution_count":50,"outputs":[{"output_type":"stream","name":"stdout","text":["Train acc: 0.9956656346749226\n"]}]},{"cell_type":"code","source":["Ptest = clf.predict(test_text_int)\n","print(f\"